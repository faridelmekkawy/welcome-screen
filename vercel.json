{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "public/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/(.+\\.(?:png|jpg|jpeg|gif|webp|mp4|webm|css|js))$", "dest": "/public/$1" }
  ],
  "cleanUrls": true
}

